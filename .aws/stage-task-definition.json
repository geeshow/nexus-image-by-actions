{
    "family": "deca-user-task",
    "taskRoleArn": "arn:aws:iam::400116879691:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::400116879691:role/ecsTaskExecutionRole",
    "volumes": [
        {
            "name": "deca-user-vol",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-065eb5e412ecbc153",
                "rootDirectory": "/deca-user",
                "transitEncryption": "DISABLED",
                "authorizationConfig": {
                    "accessPointId": "",
                    "iam": "DISABLED"
                }
            }
        }
    ],
    "containerDefinitions": [
        {
            "name": "deca-user-container",
            "cpu": "256",
            "memory": "512",
            "startTimeout": 120,
            "stopTimeout": 120,
            "portMappings": [
                {
                    "hostPort": 6010,
                    "protocol": "tcp",
                    "containerPort": 6010
                }
            ],
            "mountPoints": [
                {
                    "readOnly": false,
                    "containerPath": "/var/log",
                    "sourceVolume": "deca-user-vol"
                }
            ],
            "hostname": "",
            "essential": true,
            "disableNetworking": false,
            "privileged": true,
            "readonlyRootFilesystem": false,
            "interactive": true,
            "pseudoTerminal": true,
            "resourceRequirements": []
        }
    ],
    "networkMode": "bridge",
    "requiresCompatibilities": [
        "EC2"
    ],
    "placementConstraints": [],
    "pidMode": "task",
    "ipcMode": "task"
}